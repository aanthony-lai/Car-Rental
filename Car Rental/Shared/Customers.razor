<h3>Customers</h3>
<div class="customers-container">
	<table>
		<tr>
			<th>SSN</th>
			<th>Last Name</th>
			<th>First Name</th>
		</tr>
		@foreach (var customer in bookingProcessor.GetCustomers())
		{
			<tr>
				<td>@customer.ssn</td>
				<td>@customer.lastName</td>
				<td>@customer.firstName</td>
			</tr>
		}
	</table>
</div>

@code {
	[Inject] public BookingProcessor bookingProcessor { get; set; } 
}
